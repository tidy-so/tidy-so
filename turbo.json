{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    }
  }
}
